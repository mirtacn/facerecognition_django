#: .\venv\Lib\site-packages\django\contrib\postgres\apps.py:54
msgid "PostgreSQL extensions"
msgstr "Ekstensi PostgreSQL"

#: .\venv\Lib\site-packages\django\contrib\postgres\fields\array.py:21
#: .\venv\Lib\site-packages\django\contrib\postgres\forms\array.py:17
#: .\venv\Lib\site-packages\django\contrib\postgres\forms\array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Butir %(nth)s dalam larik tidak tervalidasi:"

#: .\venv\Lib\site-packages\django\contrib\postgres\fields\array.py:22
msgid "Nested arrays must have the same length."
msgstr "Array bersaran harus mempunyai panjang yang sama."

#: .\venv\Lib\site-packages\django\contrib\postgres\fields\hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Pemetaan dari string ke string/null"

#: .\venv\Lib\site-packages\django\contrib\postgres\fields\hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Nilai dari “%(key)s” bukan sebuah string atau null."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Could not load JSON data."
msgstr "Tidak dapat memuat data JSON."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Masukan harus kamus JSON."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:42
msgid "Enter two valid values."
msgstr "Masukkan dua nilai yang valid."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Awal jangkauan harus tidak melebihi akhir jangkauan."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:99
msgid "Enter two whole numbers."
msgstr "Masukkan dua buah bilangan bulat."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:105
msgid "Enter two numbers."
msgstr "Masukkan dua buah bilangan."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:111
msgid "Enter two valid date/times."
msgstr "Masukan dua buah tanggal/waktu."

#: .\venv\Lib\site-packages\django\contrib\postgres\forms\ranges.py:117
msgid "Enter two valid dates."
msgstr "Masukan dua buah tanggal yang benar."

#: .\venv\Lib\site-packages\django\contrib\postgres\validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Daftar mengandung %(show_value)d butir, seharusnya mengandung tidak lebih "
"dari %(limit_value)d."

#: .\venv\Lib\site-packages\django\contrib\postgres\validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Daftar mengandung%(show_value)d butir, seharusnya mengandung tidak kurang "
"dari %(limit_value)d."

#: .\venv\Lib\site-packages\django\contrib\postgres\validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Beberapa kunci hilang: %(keys)s"

#: .\venv\Lib\site-packages\django\contrib\postgres\validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Beberapa kunci yang tidak dikenali diberikan: %(keys)s"

#: .\venv\Lib\site-packages\django\contrib\postgres\validators.py:81
#, python-format
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Pastikan bahwa batas atas jangkauan tidak lebih besar dari %(limit_value)s."

#: .\venv\Lib\site-packages\django\contrib\postgres\validators.py:90
#, python-format
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr "Pastikan batas bawah jangkauan tidak kurang dari %(limit_value)s."
